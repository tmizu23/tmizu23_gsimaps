	<name>作並温泉</name>
	<ExtendedData><SchemaData schemaUrl="#OGRGeoJSON">
		<SimpleData name="_markerType">Icon</SimpleData>
		<SimpleData name="_iconUrl">http://cyberjapandata.gsi.go.jp/portal/sys/v4/symbols/002.png</SimpleData>
		<SimpleData name="_iconSize">(2:20,20)</SimpleData>
